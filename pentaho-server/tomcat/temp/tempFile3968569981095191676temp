String bagCreationField;
String operDateTimeField;
Integer interval;

Long difference = 0l;
Long bagId = 0l;
String bagCreationDateString = null;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // Get a row from the default input hop
	//
	Object[] r = getRow();

    // If the row object is null, we are done processing.
	//
	if (r == null) {
		setOutputDone();
		return false;
	}

	// Look up parameters only once for performance reason.
	//
	if (first) {
		monthDiffField = getParameter("MONTH_DIFF_FIELD");
		newDiffField = getParameter("CREATION_MONTH_DIFF_FIELD");
		bagIdField = getParameter("BAG_ID_FIELD");
        bagCreationField = getParameter("BAG_CREATION_FIELD");
        operDateTimeField = getParameter("OPER_DATE_TIME_FIELD");
        interval = Integer.valueOf(getParameter("NEW_BAG_INTERVAL"));
	    first=false;
	}

    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
	//
    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());
      
	String monthDiffString = get(Fields.In, monthDiffField).getString(r);
	String creationDateString  = get(Fields.In, operDateTimeField).getString(r);
	if (bagCreationDateString == null) {
		bagCreationDateString = creationDateString;
	}

    if (monthDiffString != null) {
		Integer monthDiff = Integer.valueOf(monthDiffString.trim());
		difference = difference + monthDiff;
    }
    
    long value = 0;

    if (difference &gt;= interval) {
       difference = 0l;
       bagId ++;
       bagCreationDateString = creationDateString;
    }

    value = difference;

	// Set the values of the output fields
	get(Fields.Out, newDiffField).setValue(outputRow, value);
    get(Fields.Out, bagIdField).setValue(outputRow, bagId);
    get(Fields.Out, bagCreationField).setValue(outputRow, bagCreationDateString);    

    // putRow will send the row on to the default output hop.
    putRow(data.outputRowMeta, outputRow);

	return true;
}</class_source>
      </definition>
