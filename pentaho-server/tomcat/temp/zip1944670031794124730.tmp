     {
      "name": "resourceCode",
      "type": "Resource",
      "value": "// init default values\nvar date = new Date();\nvar tzoffset = date.getTimezoneOffset() * 60000;\nvar day = date.getDate();\nvar month = date.getMonth(); // months from 0-11\nvar year = date.getFullYear();\nvar iii = 0;\n\nfunction formatDate(date) {\n\treturn (new Date(date - tzoffset)).toISOString().slice(0, 10);\n}\n\nfunction filterOlapData(data){\n    var rez = new Array();\n    for(var i = 0; i < data.length; i++){\n        var add = true;\n        for (var j = (i+1); j < data.length; j++) {\n          if(data[j].indexOf(data[i]) == 0){\n              add = false;\n              break;\n          }\n        }\n        if(add){\n          rez.push(data[i]);\n        }  \n    }\n    return rez;\n}\n\nfunction parseRangeFilter(str) {\n    var single = '';\n    var range = '';\n\n    str.trim().split(';').forEach(function(item, i, arr) {\n      if (item.length > 0){\n        if (item.indexOf('-') > -1) {\n          var it = item.split('-');\n          range += it[0] + ';' + it[1] + ';';\n        } else {\n          single += item + ';';\n        }\n      }  \n    });\n\n    return [single, range];\n}\n\nfunction checkDateRangeFilters() {\n    var errorMessages = [];\n    function checkDateRangeFilter(name, id, filterFrom, filterTo) {\n        var date_from = new Date(Dashboards.getParameterValue(filterFrom));\n        var date_to = new Date(Dashboards.getParameterValue(filterTo));\n        if (date_from > date_to) {\n            errorMessages.push(name + ': \"Дата с\" не может быть больше \"Даты по\".');\n            $(id + ' input').css('color', 'red');\n            $(id).fadeIn(100).fadeOut(100).fadeIn(100).fadeOut(100).fadeIn(100);\n        }\n    }\n    \n    checkDateRangeFilter('Отчётный период', '#date_filter', 'date_from', 'date_to');\n    \n    if (errorMessages.length > 0) {\n        alert(errorMessages.join(\"\\n\"));\n        return false;\n    }\n    \n    return true;\n}\n "
     },
