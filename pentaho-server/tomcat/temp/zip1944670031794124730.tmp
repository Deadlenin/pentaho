   {
    "id": "f3689cbf-45cd-445e-99eb-7bd3d15a9cf1",
    "meta": "CDA",
    "meta_conntype": "sql.jndi",
    "meta_datype": "sql",
    "parent": "SQL",
    "properties": [
     {
      "name": "name",
      "type": "Id",
      "value": "next_days_forecast_ds"
     },
     {
      "name": "access",
      "type": "Access",
      "value": "public"
     },
     {
      "name": "jndi",
      "type": "Jndi",
      "value": "vertica"
     },
     {
      "name": "query",
      "type": "SqlQuery",
      "value": "select \r\n  forecast_date as 'День составления прогноза', \r\n  dim_fps.post_object as 'Объект',\r\n  index as 'Индекс', \r\n  mt.name as 'Вид отправлений', \r\n  prev_d7 as 'Факт: в День прогноза -6д',\r\n  prev_d6 as 'Факт: в День прогноза -5д',\r\n  prev_d5 as 'Факт: в День прогноза -4д',\r\n  prev_d4 as 'Факт: в День прогноза -3д',\r\n  prev_d3 as 'Факт: в День прогноза -2д',\r\n  prev_d2 as 'Факт: в День прогноза -1д',\r\n  prev_d1 as 'Факт: в День составления прогноза',\r\n  forecast_d1 as 'План: в День прогноза +1д', \r\n  forecast_d2 as 'План: в День прогноза +2д', \r\n  forecast_d3 as 'План: в День прогноза +3д', \r\n  forecast_d4 as 'План: в День прогноза +4д', \r\n  forecast_d5 as 'План: в День прогноза +5д', \r\n  forecast_d6 as 'План: в День прогноза +6д', \r\n  forecast_d7 as 'План: в День прогноза +7д'\r\nfrom tracking.next_days_forecast ndf\r\nleft join dicts.dim_fps_structure dim_fps on ndf.index = dim_fps.post_object_index\r\nleft join dicts.mail_type_groups mt on ndf.mail_type = mt.code\r\nwhere\r\n('' in (${fps}) or 'fps' in (${fps})\r\n        \tor '[fps].[' || dim_fps.macroregion || ']' in (${fps})\r\n\t\t\tor '[fps].[' || dim_fps.macroregion || '].[' || dim_fps.ufps || ']' in (${fps})\r\n\t\t\tor '[fps].[' || dim_fps.macroregion || '].[' || dim_fps.ufps || '].[' || dim_fps.post_object || ']' in (${fps}))\r\nand ('' in (${mail_type}) or 'mail_type' in (${mail_type})\r\n\t\t\tor '[mail_type].[' || mt.group || ']' in (${mail_type})\r\n\t\t\tor '[mail_type].[' || mt.group || '].[' || mt.name || ']' in (${mail_type}))\r\norder by index, mail_type\r\n"
     },
     {
      "name": "parameters",
      "type": "CdaParameters",
      "value": "[[\"fps\",\"fps\",\"String\",\"\"],[\"mail_type\",\"mail_type\",\"String\",\"\"]]"
