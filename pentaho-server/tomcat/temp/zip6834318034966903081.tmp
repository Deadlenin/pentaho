ing_date_to = date.toISOString().slice(0, 10);\ndate.setDate(date.getDate()- 30);\naccepting_date_from = date.toISOString().slice(0, 10);\n\nvar column = {\n    \u00270 дней\u0027: \u0027d_0\u0027,\n    \u00271 день\u0027: \u0027d_1\u0027,\n    \u00272 дня\u0027: \u0027d_2\u0027,\n    \u00273 дня\u0027: \u0027d_3\u0027,\n    \u00274 дня\u0027: \u0027d_4\u0027,\n    \u00275 дней\u0027: \u0027d_5\u0027,\n    \u00276 дней\u0027: \u0027d_6\u0027,\n    \u00277 дней\u0027: \u0027d_7\u0027,\n    \u00278 дней\u0027: \u0027d_8\u0027,\n    \u00279 дней\u0027: \u0027d_9\u0027,\n    \u002710 дней\u0027: \u0027d_10\u0027,\n    \u002711-30 дней\u0027: \u0027d_11\u0027,\n    \u0027\u003E 30 дней\u0027: \u0027d_30\u0027,\n    \u0027Временное хранение\u0027: \u0027tmp_storage\u0027,\n    \u0027Всего\u0027: \u0027d_all\u0027\n};\n\nfunction filterOlapData(data){\n    var rez = new Array();\n    for(var i = 0; i \u003C data.length; i++){\n        var add = true;\n        for (var j = (i+1); j \u003C data.length; j++) {\n          if(data[j].indexOf(data[i]) == 0){\n              add = false;\n              break;\n          }\n        }\n        if(add){\n          rez.push(data[i]);\n        }  \n    }\n    return rez;\n}\n\nfunction parseRangeFilter(str) {\n    var single = \u0027\u0027;\n    var range = \u0027\u0027;\n\n    str.trim().split(\u0027;\u0027).forEach(function(item, i, arr) {\n      if (item.length \u003E 0){\n        if (item.indexOf(\u0027-\u0027) \u003E -1) {\n          var it = item.split(\u0027-\u0027);\n          range += it[0] + \u0027;\u0027 + it[1] + \u0027;\u0027;\n        } else {\n          single += item + \u0027;\u0027;\n        }\n      }  \n    });\n\n    return [single, range];\n} \n\n$(document).ready(function () {\n    $(\u0027#balances_dc_list_table\u0027).on(\u0027cdfTableComponentFinishRendering\u0027, function () {\n        let balancesDcListTable = $(\u0027#balances_dc_list_table .exportButton:first\u0027);\n        balancesDcListTable.off();//remove old onclick handler\n        balancesDcListTable.click(function () {\n            function getLatesUsedDrill() {\n                let rendered_dc = Dashboards.components.filter(function(elem){\n                        return elem.name.includes(\u0022render_balances_dc\u0022)\n                    });\n                rendered_dc.sort(function (a, b) {\n                    // Turn your strings into dates, and then subtract them\n                    // to get a value that is either negative, positive, or zero.\n                    return new Date(b.timerStart) - new Date(a.timerStart);\n                });\n                return rendered_dc[0];\n            }\n\n            let tableComponent = getLatesUsedDrill()\n            let xls_max_rows = 65000;\n            let overrideParameters = {};\n            let filterSettings = {};\n            let bigExportStrategy = tableComponent.bigExport || \u0027Dynamic\u0027;\n            let doBigExport = (bigExportStrategy == \u0027Dynamic\u0027 \u0026\u0026 parseInt(tableComponent.rawData.queryInfo.totalRows) + 1 \u003E xls_max_rows)\n                || (bigExportStrategy == \u0027Always\u0027);\n            if (doBigExport) {\n                // Set high limit to remove restriction on number of result rows\n                overrideParameters = [[\u0027limit\u0027, \u0027100000000000\u0027]];\n                tableComponent.queryState.exportBigData(overrideParameters, filterSettings, function () {\n                    alert(\u0027Запрос на выгрузку большого объема данных принят.\\n\\nВыгрузка будет произведена в формате CSV, ссылка на скачивание данных будет отправлена почтой.\u0027);\n                });\n            } else {\n                tableComponent.queryState.exportData(\u0027csv\u0027, overrideParameters, filterSettings);\n            }\n        })\n    });\n}); "},{"name":"resourceType","type":"Label","value":"Javascript"}],"type":"LayoutResourceCode","typeDesc":"Resource"},{"id":"1061ce6e-cbc5-a570-ee2f-5fb5