;\n  $(\u0027#panel_charts\u0027).show();\n}\n\nfunction checkDateRangeFilters() {\n    var errorMessages = [];\n    function checkDateRangeFilter(name, id, filterFrom, filterTo) {\n        var date_from = new Date(Dashboards.getParameterValue(filterFrom));\n        var date_to = new Date(Dashboards.getParameterValue(filterTo));\n        if (date_from \u003E date_to) {\n            errorMessages.push(name + \u0027: \u0022Дата с\u0022 не может быть больше \u0022Даты по\u0022.\u0027);\n            $(id + \u0027 input\u0027).css(\u0027color\u0027, \u0027red\u0027);\n            $(id).fadeIn(100).fadeOut(100).fadeIn(100).fadeOut(100).fadeIn(100);\n        }\n    }\n    \n    checkDateRangeFilter(\u0027Отчётный период\u0027, \u0027#date_filter\u0027, \u0027date_from\u0027, \u0027date_to\u0027);\n    checkDateRangeFilter(\u0027Предыдущий период\u0027, \u0027#prev_date_filter\u0027, \u0027prev_date_from\u0027, \u0027prev_date_to\u0027);\n    \n    if (errorMessages.length \u003E 0) {\n        alert(errorMessages.join(\u0022\\n\u0022));\n        return false;\n    }\n    \n    return true;\n}\n\nfunction exportTables(tables) {\n  var tableFilenames = {\n    \u0027cvpp_drill_number_arrival_25h_table\u0027: \u0027Переданы из СЦ на ЦВПП. Обработаны позднее 24 часов\u0027,\n    \u0027cvpp_drill_number_await_arrival_25h_table\u0027: \u0027Переданы из СЦ на ЦВПП. Не обработаны свыше 24 часов\u0027,\n    \u0027cvpp_drill_in_ops_after_accepting_25h_table\u0027: \u0027Переданы из ЦВПП на СЦ с момента приема позднее 24 часов\u0027,\n    \u0027cvpp_drill_in_ops_after_accepting_table\u0027: \u0027Не переданы из ЦВПП на СЦ\u0027,\n    \u0027cvpp_drill_await_delivery_total_table\u0027: \u0027Ожидающие вручения. Итого\u0027,\n    \u0027cvpp_drill_await_delivery_8d_table\u0027: \u0027Ожидающие вручения от 8 и более дней\u0027,\n    \u0027cvpp_drill_arriving_delivery_7d_table\u0027: \u0027Врученные до 7 дней\u0027,\n    \u0027cvpp_drill_arriving_delivery_8d_table\u0027: \u0027Врученные от 8